{
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "robmorgan/phinx": "^0.16.10",
    "friendsofphp/php-cs-fixer": "^3.89",
    "nunomaduro/phpinsights": "^2.12"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "lint": "vendor/bin/php-cs-fixer fix --dry-run --diff",
    "format": "vendor/bin/php-cs-fixer fix",
    "insights": "php vendor/bin/phpinsights -n",
    "migrate-init": "vendor/bin/phinx create InitInfraSpassu",
    "migrate": "vendor/bin/phinx migrate",
    "migrate-back": "vendor/bin/phinx rollback -t 0",
    "migrate-init-data": "vendor/bin/phinx seed:create AntenasSeeder",
    "migrate-load-data": "vendor/bin/phinx seed:run -s AntenasSeeder"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require": {
    "php": ">=8.2.29.",
    "twig/twig": "^3.0",
    "twbs/bootstrap": "^5.3"
  }
}
